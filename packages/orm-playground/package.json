	"author": "Aleksandar Stanisic <aleksandar@tuta.io>",
	"homepage": "https://github.com/apstanisic/zmaj#readme",
	"license": "MIT",
	"prettier": "../../.prettierrc.json",
	"repository": {
		"type": "git",
		"url": "https://github.com/apstanisic/zmaj.git"
	},
	"bugs": {
		"url": "https://github.com/apstanisic/zmaj/issues"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"type": "module",
	"private": true,
	"name": "@zmaj-js/orm-playground",
	"version": "0.2.2",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"eslintConfig": {
		"extends": "../../.eslintrc.cjs"
	},
	"scripts": {
		"prebuild": "rimraf dist",
		"lint": "eslint --fix src",
		"test": "cd ../.. && vitest run --config vitest-unit.config.ts projects/orm-playground",
		"tsc": "tsc",
		"build": "tsup --config ../../tsup.config.ts",
		"dev": "tsup --config ../../tsup.config.ts --watch",
		"format": "prettier . --write --ignore-unknown  --ignore-path ../../.prettierignore"
	},
	"dependencies": {
		"@zmaj-js/orm": "workspace:*",
		"@zmaj-js/orm-sq": "workspace:*",
		"type-fest": "^4.3.1"
	},
	"devDependencies": {
		"@zmaj-js/test-utils": "workspace:*"
	}
}
