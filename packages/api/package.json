	"eslintConfig": {
		"extends": "../../.eslintrc.cjs"
	},
	"scripts": {
		"prebuild": "rimraf dist",
		"lint": "eslint --fix src",
		"test": "cd ../.. && vitest run packages/api --passWithNoTests",
		"test-e2e-api": " vitest run --config vitest.config.e2e.ts",
		"test-e2e-api-watch": " vitest watch --config vitest.config.e2e.ts",
		"tsc": "tsc",
		"build": "tsup --config tsup.config.ts",
		"dev": "tsup --config tsup.config.ts --watch",
		"serve": "NODE_ENV=dev node --inspect --watch dist/main.js",
		"format": "prettier . --write --ignore-unknown  --ignore-path ../../.prettierignore",
		"test-watch": "cd ../.. && vitest watch packages/api --passWithNoTests"
	},
